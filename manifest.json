{
  "manifest_version": 3,
  "name": "Red night moon",
  "version": "0.0.1",
  "description": "這是一個Red night moon Team的擴充",
  "icons": {
    "128": "icons/icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
